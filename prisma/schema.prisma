// prisma/schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite" 
  url      = env("DATABASE_URL")
}

model Menu {
  id        Int      @id @default(autoincrement())
  name      String
  price     Int
  category  String   
  imagePath String
  createdAt DateTime @default(now())
}


